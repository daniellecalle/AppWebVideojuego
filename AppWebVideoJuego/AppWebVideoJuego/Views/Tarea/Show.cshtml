
@{
	ViewBag.Title = "Show";
	AppWebVideoJuego.Models.Tarea model = (AppWebVideoJuego.Models.Tarea)ViewData["model"];
}


<div class="panel panel-info">
	<div class="panel-body">
		<p><b>ID: </b> @Html.Raw(model.GetId())</p>
		<p><b>Nombre: </b> @Html.Raw(model.GetNombre())</p>
		<p><b>Descripcion: </b> @Html.Raw(model.GetDescripcion())</p>
		<p><b>Equipo: </b> @Html.Raw(model.GetIdEquipo())</p>
	</div>
</div>

@Html.ActionLink("Atras", "ShowTareas", "Tarea", new { area = "" }, new { @class = "btn btn-success" })